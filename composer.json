{
	"name": "wulicode/poppy",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.2",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-openssl": "*",
		"ext-pdo": "*",
		"ext-gd": "*",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-bcmath": "*",
		"ext-fileinfo": "*",
		"ext-libxml": "*",
		"ext-xml": "*",
		"jenssegers/date": "^4.0",
		"laravel/framework": "6.0.*",
		"maatwebsite/excel": "^3.0",
		"intervention/image": "2.*",
		"laravelcollective/html": "^6.0",
		"mews/captcha": "3.*",
		"predis/predis": "~1.1",
		"guzzlehttp/guzzle": "6.*",
		"erusev/parsedown": "^1.7",
		"sunra/php-simple-html-dom-parser": "1.5.2",
		"aliyuncs/oss-sdk-php": "^2.3",
		"comodojo/xmlrpc": "^1.1",
		"symfony/yaml": "5.*",
		"centrifugal/phpcent": "~3.0",
		"tucker-eric/eloquentfilter": "^2",
		"tymon/jwt-auth": "1.0.*",
		"hieu-le/active": "4.0.*",
		"php-curl-class/php-curl-class": "8.*",
		"alibabacloud/client": "^1.5",
		"alibabacloud/push": "^1.8",
		"alibabacloud/dysmsapi": "^1.8",
		"alibabacloud/sts": "^1.8",
		"overtrue/pinyin": "^4.0"
	},
	"require-dev": {
		"itsgoingd/clockwork": "~4.0",
		"barryvdh/laravel-ide-helper": "~2.7",
		"doctrine/dbal": "^2.5"
	},
	"autoload": {
		"classmap": [
		],
		"psr-4": {
			"Poppy\\Extension\\Alipay\\": "poppy/ext-alipay/src",
			"Poppy\\Extension\\Alipay\\Tests\\": "poppy/ext-alipay/tests",
			"Poppy\\Extension\\Wxpay\\": "poppy/ext-wxpay/src",
			"Poppy\\Extension\\Wxpay\\Tests\\": "poppy/ext-wxpay/tests",
			"Poppy\\Extension\\Netease\\": "poppy/ext-netease/src",
			"Poppy\\Extension\\Netease\\Tests\\": "poppy/ext-netease/tests",
			"Poppy\\Extension\\IpStore\\": "poppy/ext-ip_store/src",
			"Poppy\\Extension\\IpStore\\Tests\\": "poppy/ext-ip_store/tests",
			"Poppy\\Extension\\Pinyin\\": "poppy/ext-pinyin/src",
			"Poppy\\Extension\\Pinyin\\Tests\\": "poppy/ext-pinyin/tests",
			"Poppy\\Faker\\": "poppy/faker/src",
			"Poppy\\Faker\\Tests\\": "poppy/faker/tests",
			"Poppy\\Framework\\": "poppy/framework/src",
			"Poppy\\Framework\\Tests\\": "poppy/framework/tests",
			"Poppy\\Core\\": "poppy/core/src",
			"Poppy\\Core\\Tests\\": "poppy/core/tests",
			"Poppy\\Demo\\": "poppy/demo/src",
			"Poppy\\Demo\\Tests\\": "poppy/demo/tests",
			"Poppy\\System\\": "poppy/system/src",
			"Poppy\\System\\Tests\\": "poppy/system/tests",
			"Poppy\\MgrPage\\": "poppy/mgr-page/src/",
			"Poppy\\MgrPage\\Tests\\": "poppy/mgr-page/tests/",
			"Poppy\\Ad\\": "poppy/ad/src",
			"Poppy\\Ad\\Tests\\": "poppy/ad/tests",
			"Poppy\\Area\\": "poppy/area/src",
			"Poppy\\Area\\Tests\\": "poppy/area/tests",
			"Poppy\\Sms\\": "poppy/sms/src",
			"Poppy\\Sms\\Tests\\": "poppy/sms/tests",
			"Poppy\\AliyunPush\\": "poppy/aliyun-push/src",
			"Poppy\\AliyunPush\\Tests\\": "poppy/aliyun-push/tests",
			"Poppy\\AliyunOss\\": "poppy/aliyun-oss/src",
			"Poppy\\AliyunOss\\Tests\\": "poppy/aliyun-oss/tests",
			"Php\\Tests\\": "modules/php/tests",
			"Site\\Tests\\": "modules/site/tests"
		},
		"files": [
			"poppy/framework/src/Support/functions.php",
			"poppy/system/src/Support/functions.php",
			"poppy/core/src/Support/functions.php",
			"poppy/ext-pinyin/src/Support/functions.php"
		]
	},
	"autoload-dev": {
		"classmap": [
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": [
				"laravelcollective/html"
			]
		}
	},
	"scripts": {
		"py": [
			"composer run mgr-page",
			"composer run system",
			"composer run framework",
			"composer run core",
			"composer run sms",
			"composer run faker",
			"composer run ext-wxpay",
			"composer run ext-alipay",
			"composer run ext-pinyin",
			"composer run ext-netease",
			"composer run ext-ip_store",
			"composer run demo",
			"composer run area",
			"composer run aliyun-push",
			"composer run aliyun-oss",
			"composer run ad"
		],
		"py-github": [
			"composer run gh-mgr-page",
			"composer run gh-system",
			"composer run gh-framework",
			"composer run gh-core"
		],
		"ad": [
			"git push ad `git subtree split --prefix=poppy/ad 3.0`:3.0 --force"
		],
		"aliyun-oss": [
			"git push aliyun-oss `git subtree split --prefix=poppy/aliyun-oss 3.0`:3.0 --force"
		],
		"aliyun-push": [
			"git push aliyun-push `git subtree split --prefix=poppy/aliyun-push 3.0`:3.0 --force"
		],
		"area": [
			"git push area `git subtree split --prefix=poppy/area 3.0`:3.0 --force"
		],
		"core": [
			"git push core `git subtree split --prefix=poppy/core 3.0`:3.0 --force"
		],
		"demo": [
			"git push demo `git subtree split --prefix=poppy/demo 3.0`:3.0 --force"
		],
		"ext-alipay": [
			"git push ext-alipay `git subtree split --prefix=poppy/ext-alipay 3.0`:3.0 --force"
		],
		"ext-ip_store": [
			"git push ext-ip_store `git subtree split --prefix=poppy/ext-ip_store 3.0`:3.0 --force"
		],
		"ext-netease": [
			"git push ext-netease `git subtree split --prefix=poppy/ext-netease 3.0`:3.0 --force"
		],
		"ext-pinyin": [
			"git push ext-pinyin `git subtree split --prefix=poppy/ext-pinyin 3.0`:3.0 --force"
		],
		"ext-wxpay": [
			"git push ext-wxpay `git subtree split --prefix=poppy/ext-wxpay 3.0`:3.0 --force"
		],
		"faker": [
			"git push faker `git subtree split --prefix=poppy/faker 3.0`:3.0 --force"
		],
		"framework": [
			"git push framework `git subtree split --prefix=poppy/framework 3.0`:3.0 --force"
		],
		"mgr-page": [
			"git push mgr-page `git subtree split --prefix=poppy/mgr-page 3.0`:3.0 --force"
		],
		"project": [
			"git push project `git subtree split --prefix=poppy/project 3.0`:3.0 --force"
		],
		"sms": [
			"git push sms `git subtree split --prefix=poppy/sms 3.0`:3.0 --force"
		],
		"system": [
			"git push system `git subtree split --prefix=poppy/system 3.0`:3.0 --force"
		],
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate"
		],
		"pre-update-cmd": [
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan ide-helper:meta"
		],
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
